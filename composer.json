{
  "name": "drupal/scheduler_content_moderation_integration",
  "type": "drupal-module",
  "description": "Scheduler content moderation integration",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "require": {
    "drupal/scheduler": "1.0"
  },
  "extra": {
    "patches": {
      "drupal/scheduler": {
        "Scheduler integration with core Content moderation": "https://www.drupal.org/files/issues/2019-04-23/2798689-202.content_moderation_hooks.patch",
        "Load latest revision of node when publishing and unpublishing": "https://www.drupal.org/files/issues/2019-04-18/3049070-1.load_latest_revision.patch",
        "Replace assertRaw(t()) with assertText()": "https://www.drupal.org/files/issues/2019-02-07/3031199-3.change_assertRaw_to_assertText.patch"
      }
    }
  }
}
